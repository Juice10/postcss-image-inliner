    "debug": "^4.3.4",
    "filesize": "^9.0.11",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-space": "^0.33.0",
