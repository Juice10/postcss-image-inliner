  "license": "MIT",
  "repository": "bezoerb/postcss-image-inliner",
  "bugs": {
    "url": "https://github.com/bezoerb/postcss-image-inliner/issues"
  },
  "homepage": "https://github.com/bezoerb/postcss-image-inliner",
  "dependencies": {
